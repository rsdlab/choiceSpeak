# data_out CMake config file
#
# This file sets the following variables:
# data_out_FOUND - Always TRUE.
# data_out_INCLUDE_DIRS - Directories containing the data_out include files.
# data_out_IDL_DIRS - Directories containing the data_out IDL files.
# data_out_LIBRARIES - Libraries needed to use data_out.
# data_out_DEFINITIONS - Compiler flags for data_out.
# data_out_VERSION - The version of data_out found.
# data_out_VERSION_MAJOR - The major version of data_out found.
# data_out_VERSION_MINOR - The minor version of data_out found.
# data_out_VERSION_REVISION - The revision version of data_out found.
# data_out_VERSION_CANDIDATE - The candidate version of data_out found.

message(STATUS "Found data_out-@PROJECT_VERSION@")
set(data_out_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(data_out_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(data_out_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(data_out_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(data_out_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(data_out_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(data_out_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(data_out_DEFINITIONS ${<dependency>_DEFINITIONS})

set(data_out_VERSION @PROJECT_VERSION@)
set(data_out_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(data_out_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(data_out_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(data_out_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

